      "@types/koa-bodyparser": "^4.3.0",
      "@types/koa-multer": "^1.0.0",
      "@types/koa-router": "^7.4.1",
      "@types/moment": "^2.13.0",
      "@types/node": "^14.0.4",
      "@types/winston": "^2.4.4",
      "bcrypt": "^4.0.1",
      "class-transformer": "^0.2.3",
      "class-validator": "^0.12.2",
      "dotenv": "^8.2.0",
      "http": "0.0.1-security",
      "jsonwebtoken": "^8.5.1",
      "kcors": "^2.2.2",
      "koa": "^2.12.0",
      "koa-bodyparser": "^4.3.0",
      "koa-multer": "^1.0.2",
      "koa-router": "^8.0.8",
      "moment": "^2.26.0",
      "mysql": "^2.14.1",
      "nodemon": "^2.0.4",
      "pg": "^8.2.1",
      "prettier": "^2.0.5",
      "reflect-metadata": "^0.1.10",
      "routing-controllers": "^0.8.1",
      "socket.io": "^2.5.0",
      "socketio-jwt-auth": "^0.1.0",
      "ts-node": "^8.10.1",
      "typeorm": "0.2.25",
      "typescript": "^3.9.3",
      "winston": "^3.2.1"
   },
   "engines": {
      "node": "10.x"
   }
